<customUI xmlns="http://schemas.microsoft.com/office/2009/07/customui" onLoad="RibbonOnLoad" > 	
	<ribbon startFromScratch="false">
		<contextualTabs>
			<tabSet idMso="TabSetSmartArtTools" visible="true" />
			<tabSet idMso="TabSetChartTools" visible="true" />
			<tabSet idMso="TabSetDrawingTools" visible="true" />
			<tabSet idMso="TabSetPictureTools" visible="true" />
			<tabSet idMso="TabSetPivotTableTools" visible="true" />
			<tabSet idMso="TabSetHeaderAndFooterTools" visible="true" />
			<tabSet idMso="TabSetTableToolsExcel" visible="true" />
			<tabSet idMso="TabSetPivotChartTools" visible="true" />
			<tabSet idMso="TabSetInkTools" visible="true" />
		</contextualTabs>
		<tabs>
			<tab id="CustomTab" insertBeforeMso="TabHome" label="DPR Reporter"> 
				<group  id="Group1" getLabel="getLabel0">
     					<labelControl id="Label1" getLabel="getLabel1" />
					<labelControl id="Label2" getLabel="getLabel2" />
					<labelControl id="Label3" getLabel="getLabel3" tag="Lbl3" />
				</group>

				<group id="Grp2A" label="Build Reports">
					<button id="g2a1a" label="Build Reports" imageMso="OpenInfopathForm" size="large" onAction="g2b1a" />
					<separator id="g2As1" />
					<button id="g2a1c" label="Manage Markups" imageMso="OutlineSettings" size="large" onAction="g2b1c" /> 
					<separator id="g2s1" />
					<button id="g2b1" label="Report Options" imageMso="Grouping" size="large" onAction="g2b2" />		
				</group>

				<group id="Grp2" label="Workbook Controls">
					<menu id="g4m1" label="DPR Custom Themes" imageMso="ThemesGallery" size="large" itemSize="large" >
						<button id="g4m1b1" label="Standard Light Theme" imageMso="AppointmentColorDialog" onAction="g4m1b1" />
						<button id="g4m1b2" label="Standard Dark Theme" imageMso="AppointmentColorDialog" onAction="g4m1b2" />
						<menuSeparator id="g4m1s1" title="DPR Market Sector Themes" />
              				<button id="g4m1b3" label="Advanced Technology" imageMso="AppointmentColorDialog" onAction="g4m1b3" />
              				<menuSeparator id="g4m1s2" />
              				<button id="g4m1b4" label="Corporate Office" imageMso="AppointmentColorDialog" onAction="g4m1b4" />
              				<menuSeparator id="g2m1s3" />
              				<button id="g4m1b5" label="Healthcare" imageMso="AppointmentColorDialog" onAction="g4m1b5" />
						<menuSeparator id="g4m1s4" />
              				<button id="g4m1b6" label="Higher Education" imageMso="AppointmentColorDialog" onAction="g4m1b6" />
						<menuSeparator id="g4m1s5" />
              				<button id="g4m1b7" label="Life Sciences" imageMso="AppointmentColorDialog" onAction="g4m1b7" />
						<menuSeparator id="g4m1s6" />
              				<button id="g4m1b8" label="Generic" imageMso="AppointmentColorDialog" onAction="g4m1b8" />
					</menu>
					<separator id="g2s4" />	
          				<menu id="formatCurrency" label="Currency Format" size="large" imageMso="DollarSign"  >
            				<button id="formatCurrency1" getLabel="getlabel10" onAction="Macro8"  />
            				<button id="formatCurrency2" getLabel="getlabel20"  onAction="Macro9"  />
            				<button id="formatCurrency3" getLabel="getlabel30" onAction="Macro10"  />
            				<button id="formatCurrency4" getLabel="getlabel40"  onAction="Macro11"  />
						<button id="formatCurrency5" getLabel="getlabel50"  onAction="Macro12"  />
						<button id="formatCurrency6" getLabel="getlabel60"  onAction="Macro13"  />
						<button id="formatCurrency7" getLabel="getlabel70"  onAction="Macro14"  />
						<button id="formatCurrency8" getLabel="getlabel80"  onAction="Macro15"  />
						<button id="formatCurrency9" getLabel="getlabel81"  onAction="Macro16"  />
          				</menu>			
					<button id="g2b2" label="Export Report" imageMso="ChartShowData" size="large" onAction="g3b3" />
					<separator id="g2s2" />
					<button id="g2b3" label="Convert Report to Table" imageMso="ChartShowData" size="large" onAction="btnSk2" />
					<separator id="g2s3" />	
					<button id="g2b4" label="Save to Local Drive" imageMso="FileSaveAsCurrentFileFormat" size="large" onAction="g2b3" />
					<separator id="g2s6" />			
					<button id="g2b4a" label="Refresh Estimate Data" imageMso="DataRefreshAll" size="large" onAction="g2b4a" />
				</group>
    		
				
				<group id="Grp4" label="Page and Print Setup">
					<button id="g4b0" label="Format Sheet" imageMso="PrintAreaMenu" size="large" onAction="g4b0" />
					<button id="g4b1" label="Print Options Menu" imageMso="PrintOptionsMenu" size="large" onAction="g4b1" />
					<button id="g2b5" label="Create PDF/XPS Document" imageMso="FileSaveAsPdfOrXps" size="large" onAction="g2b4" />
					<separator id="g4s1" />
					<toggleButton idMso="ViewNormalViewExcel" size="large" />
					<toggleButton idMso="ViewPageBreakPreviewView" size="large" />
					<menu idMso="PageBreakMenu" size="large" />
				</group>				
			</tab>
		</tabs>
	</ribbon>
	<backstage>
		<tab idMso="TabPrint" > 
			<firstColumn>	
				<group idMso="GroupPrintSettings" visible="true" /> 
				<group id="bsGroup2" label="DPR Print Options" insertBeforeMso="GroupPrintSettings" helperText="Select Multple worksheets to print as a single report.">
                    		<primaryItem>
		   				<button id="bsBtn1" label="Print Options Menu" imageMso="PrintOptionsMenu"  onAction="g4b1" />

					</primaryItem> 
                		</group> 
			</firstColumn> 
		</tab> 
	</backstage> 
</customUI>